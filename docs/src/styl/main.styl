@import '../../../src/themes/default/kdfn.styl'

body
  font-family               'Open Sans', 'sans-serif'

section.home-section
  position                  relative
  overflow                  visible


.inner-wrapper
  rel()
  max-width                 1290px
  padding                   0 45px
  margin                    0px auto
  overflow                  visible
  border-top                1px solid transparent
  borderBox()


.kdtabview
  .kdtabhandlecontainer
    bg                      color, transparent
    border                  none
    height                  36px
    overflow                visible

    .kdtabhandle-tabs
      overflow              visible

    .kdtabhandle
      height                36px
      line-height           36px
      rounded               4px 4px 0 0
      margin-right          3px
      border                none
      padding               0 24px
      color                 #858585
      font-size             14px
      background            #ececec

      &.active
        background          #333
        color               #f1f1f1

  pre.kdtabpaneview
    rounded                 0 4px 4px 4px
    background              #333
    padding                 20px
    borderBox()

    &:before
    &:after
      display               none

    code
      font-family           'Courier New','Courier',monospace, sans-serif
      width                 auto
      background            transparent
      borderBox()


header.main-header
  bg                        color, #333
  bg                        image, url('data:image/png;base64,')
  bg                        repeat, repeat
  bg                        scroll, no-scroll
  size                      100%, 100px
  top                       0px
  left                      0px
  z-index                   8
  fix()

  figure.logo
    size                    64px 50px
    abs                     23px 0 0 30%
    background              url('../src/img/logo.svg') no-repeat center
    background-size         contain
    opacity                 0
    margin-left             45px
    vendor                  transition, left .3s ease \,\
                                        opacity .3s ease

  nav
    abs                     29px 50% 0 0
    vendor                  transform, translateX(50%)
    width                   auto
    -webkit-transition      -webkit-transform .3s ease \,
                            right .3s ease

    -moz-transition         -moz-transform .3s ease \,
                            right .3s ease

    a
      color                 #979797
      font-size             14px
      display               inline-block
      line-height           36px
      vertical-align        middle
      noTextDeco()

      &~a
        margin              0 0 0 9px

      &:hover
        text-decoration     underline

      &#fork-us
        border              2px solid #525252
        rounded             6px
        padding             0 25px 0 0
        font-weight         600
        margin-left         23px
        width               189px
        white-space         nowrap
        overflow            hidden
        vendor              transition, color .2s .1s ease \,
                                        width .2s .1s ease

        &:hover
          bg                color, rgba(255, 255, 255, .07)
          noTextDeco()

        &:before
          content           ''
          display           block
          size              19px, 16px
          bg                image, url('../src/img/fork.svg')
          bg                position, center center
          bg                repeat, no-repeat
          bg                size, 19px 16px
          margin            10px 14px 0
          fl()

      &.download-button
        bg                  image, url('../src/img/download-grey.svg')
        bg                  position, 18px 9px
        bg                  repeat, no-repeat
        bg                  size, 17px 17px
        border              2px solid #525252
        rounded             6px
        height              36px
        width               53px
        margin-right        -53px
        opacity             0
        vendor              transition, opacity .3s ease

        &:hover
          bg                color, rgba(255, 255, 255, .07)
          vendor            animation, downloadIcon .4s 1 ease


.main-view.collapse-header
.page-view
  .main-header
    shadow                  0 4px 12px rgba(0, 0, 0, .3)

    .download-button
      opacity               1

    figure.logo
      left                  0%
      opacity               1

    nav
      right                 98px
      vendor                transform, translateX(0)

      a#fork-us
        width               24px
        color               transparent


section.home-section
  background                #fff
  z-index                   3

  h2
  h3
    border                  none
    overflow                visible

  &.intro
    padding                 29px 0 168px 0
    bg                      color, #333
    bg                      image, url('data:image/png;base64,')
    bg                      repeat, repeat
    bg                      scroll, no-scroll
    text-align              center
    size                    100%, auto
    z-index                 2
    shadow                  0 0 0 rgba(0,0,0,.4)
    fix()

    figure.logo
      size                  188px 148px
      margin                122px auto 0
      display               block
      background            url('../src/img/logo.svg') no-repeat center
      background-size       contain

    h2
      text-align            center
      margin                35px 0 93px 0

      span
        font-size           36px
        color               #fff
        font-weight         300

    .download-button
      display               inline-block
      height                60px
      line-height           60px
      background            #ff5a27
      rounded               30px
      font-size             20px
      color                 #effee5
      padding               0 0 0 27px
      overflow              hidden
      width                 354px
      noTextDeco()

      &:after
        content             ''
        display             block
        size                72px, 60px
        bg                  color, #c3431c
        bg                  image, url('../src/img/download-white.svg')
        bg                  position, 16px 18px
        bg                  repeat, no-repeat
        bg                  size, 32px 24px
        margin-left         27px
        fr()

  &.features
    margin-top              630px
    shadow                  0 -12px 38px rgba(0,0,0, .75)

    &:before
      content               ''
      display               block
      size                  50%, 12px
      background            #fff
      abs                   -12px 0 0 50%
      vendor                transform, skewX(-45deg)
      margin-left           6px

    &:after
      content               ''
      display               block
      size                  50%, 12px
      background            #fff
      abs                   -12px 0 0 0px
      vendor                transform, skewX(45deg)
      margin-left           -6px

    .inner-wrapper
      vertical-align        top
      padding               102px 45px 82px
      border-bottom         2px solid #f6f6f6

    .feature
      display               inline-block
      width                 33%
      padding               0 55px 0 0
      color                 #555
      borderBox()

      figure
        background-size     contain

        &.chrome
          margin            7px 0 68px 0
          background        url('../src/img/chrome.svg') no-repeat center
          size              54px, 46px

        &.king
          margin            10px 0 67px 0
          background        url('../src/img/king.svg') no-repeat center
          size              54px, 45px

        &.box
          margin            0 0 68px 0
          background        url('../src/img/box.svg') no-repeat center
          size              47px, 54px

      h3
        font-size           16px
        font-weight         700

      p
        font-size           16px
        line-height         22px
        letter-spacing      0.02em
        padding             23px 0

  &.examples
    margin-bottom           163px

    .inner-wrapper
      padding               69px 45px 72px

    h2
      font-size             30px
      font-weight           600
      color                 #464646

    h3
      font-size             22px
      font-weight           300
      color                 #6b6b6b
      margin                8px 0 0

    .kdtabview
      width                 66%
      margin                60px 0 74px

    .go-to-doc
      font-size             22px
      color                 #6b6b6b

      a
        color               inherit


footer.kdview
  size                      100%, 163px
  background                #333333
  bottom                    0px
  left                      0px
  z-index                   1
  fix()

  .about-kd
    padding                 58px 0 0 0
    font-size               16px
    color                   #858585
    font-weight             300
    line-height             24px
    fl()

  nav
    line-height             163px
    fr()

    a
      color                 #858585
      font-weight           300
      font-size             16px
      display               inline-block
      line-height           36px
      margin-left           16px
      noTextDeco()


.guide-page.inner-wrapper
  padding-top               146px


aside.main-sidebar
  size                      252px, auto
  borderBox()
  fl()

  ul.kdlistview
    margin-bottom           24px

    .arrow
      hidden()

    >li
      cursor               pointer

    >li.has-sub-items
      display               block
      font-size             16px
      font-weight           700
      color                 #555
      line-height           17px
      margin                0 0 20px 0
      padding               0 0 0 10px
      border-left           4px solid transparent
      borderBox()

      &.expanded
        border-left-color   #ff5a27

    li:not(.has-sub-items)
      display               block
      font-size             14px
      color                 #777
      height                24px
      line-height           24px
      margin                2px 0 0 14px
      padding               0 0 0 12px
      noTextDeco()
      borderBox()

      &.selected
        background          #eeeeee
        rounded             3px

  .nav-main-item
    margin-bottom           24px

    >span
      display               block
      font-size             16px
      font-weight           700
      color                 #555555
      line-height           17px
      margin                0 0 20px 0
      padding               0 0 0 10px
      cursor                pointer
      border-left           4px solid transparent
      borderBox()

    a.nav-sub-item
      display               block
      font-size             14px
      color                 #777777
      height                24px
      line-height           24px
      margin                2px 0 0 14px
      padding               0 0 0 12px
      display               none
      noTextDeco()
      borderBox()

    &.active
      >span
        border-left-color   #ff5a27

      a.nav-sub-item
        display             block

        &.active
          background        #eee
          rounded           3px


section.content-section
  size                      auto, auto
  margin-left               252px
  borderBox()

  article
    section
      padding               0 0 42px 40px
      borderBox()

    h3
      color                 #555555
      font-size             18px
      font-weight           600
      margin                0 0 22px 0

    .edit-button
      size                  23px, 22px
      rounded               3px
      bg                    color, #f1f1f1
      border                1px solid #e5e5e5
      padding               0
      vertical-align        middle
      text-align            center
      margin-left           8px

      .icon
        display             block
        size                23px 22px
        bg                  image, url('../src/img/edit.svg')
        bg                  position, 5px 3px
        bg                  repeat, no-repeat
        bg                  size, 11px 13px

      &:hover
        bg                  color, #dfdfdf

    p
      line-height           22px
      letter-spacing        .025em
      color                 #555555
      margin                0 0 24px 0

      &:last-of-type
        margin-bottom       0

        &+.kdtabview
          margin-top        22px


@keyframes downloadIcon
  0%
    bg                      position, 18px 9px

  49%
    bg                      position, 18px 36px

  49.1%
    bg                      position, 18px -24px

  100
    bg                      position, 18px 9px
